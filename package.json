{
    "name": "explorer",
    "version": "0.0.1",
    "description": "Explorer",
    "engines": {
        "node": ">=18"
    },
    "type": "module",
    "main": "index.js",
    "scripts": {
        "eslint": "            eslint . --ext .js,.mjs --report-unused-disable-directives",
        "eslint:fix": "npm run eslint -- --fix",
        "stylelint": "                stylelint --config \"./frontend/stylelint.config.cjs\" \"frontend/src/**/*.css\"",
        "stylelint:fix": "    npm run stylelint -- --fix",
        "stylelint:verbose": "npm run stylelint -- --formatter verbose",
        "lint": "npm run eslint && npm run stylelint",
        "test": "npm run lint",
        "start:build": "MODE=development webpack --watch --config frontend/webpack.config.mjs",
        "start:server": "                              nodemon --exitcrash backend/server.mjs",
        "start:server:port-dynamic": "PORT_DYNAMIC=yes nodemon --exitcrash backend/server.mjs",
        "start:server:inspect-brk": "                  node --inspect-brk  backend/server.mjs",
        "start": "             concurrently --kill-others \"npm run start:server\"              \"npm run start:build\"",
        "start:port-dynamic": "concurrently --kill-others \"npm run start:server:port-dynamic\" \"npm run start:build\""
    },
    "dependencies": {
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
        "body-parser": "^1.20.2",
        "copy-webpack-plugin": "^11.0.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "get-port": "^7.0.0",
        "got": "=13.0.0",
        "mini-css-extract-plugin": "^2.7.6",
        "node-notifier": "^10.0.1",
        "note-down": "^1.0.2",
        "webpack": "^5.89.0",
        "webpack-dev-middleware": "^6.1.1",
        "webpack-hot-middleware": "^2.25.4"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.23.3",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.15.1",
        "@mui/material": "^5.15.1",
        "@stylistic/eslint-plugin": "^1.5.1",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "babel-loader": "^9.1.3",
        "browser-image-compression": "^2.0.2",
        "classnames": "^2.3.2",
        "concurrently": "^8.2.2",
        "css-loader": "^6.8.1",
        "dequal": "^2.0.3",
        "eslint": "^8.56.0",
        "eslint-config-ironplate": "=0.9.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unicorn": "^50.0.1",
        "file-saver": "^2.0.5",
        "fuse.js": "^7.0.0",
        "helpmate": "=1.5.13",
        "idb-keyval": "^6.2.1",
        "jotai": "^2.6.0",
        "jszip": "^3.10.1",
        "ky": "^1.1.3",
        "nodemon": "^3.0.2",
        "p-memoize": "^7.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-refresh": "^0.14.0",
        "react-virtuoso": "^4.6.2",
        "stylelint": "^16.0.2",
        "stylelint-config-css-modules": "^4.4.0",
        "stylelint-config-recommended": "^14.0.0",
        "webpack-cli": "^5.1.4"
    },
    "overrides": {
        "stylelint-config-css-modules": {
            "stylelint": "^16.0.2"
        }
    }
}
